{
  "name": "n8n-workflow-converter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "next build",
    "build:turbo": "next build --turbopack",
    "build:production": "NODE_ENV=production next build",
    "build:standalone": "BUILD_STANDALONE=true next build",
    "start": "next start",
    "start:production": "tsx scripts/start-production.ts",
    "start:standalone": "node scripts/start-standalone.js",
    "start:auto": "node scripts/start-standalone.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:complete": "playwright test src/__tests__/e2e/complete-workflow.test.ts",
    "test:e2e:collaboration": "playwright test src/__tests__/e2e/collaboration.test.ts",
    "test:e2e:errors": "playwright test src/__tests__/e2e/error-handling.test.ts",
    "test:e2e:performance": "playwright test src/__tests__/e2e/performance.test.ts",
    "test:e2e:runner": "tsx src/__tests__/e2e/test-runner.ts",
    "test:e2e:all": "npm run test:e2e:runner all",
    "test:e2e:cross-env": "npm run test:e2e:runner cross-env",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "sync-n8n": "tsx scripts/sync-n8n-source.ts",
    "sync-n8n:watch": "tsx watch scripts/sync-n8n-source.ts",
    "test:source-analysis": "jest --testPathPattern=source-analysis",
    "license-check": "license-checker --summary",
    "license-report": "license-checker --csv --out licenses.csv",
    "license-validate": "license-checker --failOn 'GPL;AGPL;LGPL'",
    "generate-licenses": "tsx scripts/generate-license-report.ts",
    "attribution-check": "tsx scripts/check-attribution.ts",
    "db:status": "tsx scripts/db-migration.ts status",
    "db:migrate": "tsx scripts/db-migration.ts apply",
    "db:migrate:dry": "tsx scripts/db-migration.ts apply --dry-run",
    "db:rollback": "tsx scripts/db-migration.ts rollback",
    "db:backup": "tsx scripts/db-migration.ts backup",
    "db:restore": "tsx scripts/db-migration.ts restore",
    "db:validate": "tsx scripts/db-migration.ts validate",
    "docker:build": "docker build -t n8n-workflow-converter .",
    "docker:run": "docker run -p 3000:3000 --env-file .env.production n8n-workflow-converter",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "lighthouse": "lhci autorun",
    "lighthouse:desktop": "lhci autorun --config=lighthouse.config.js",
    "security:scan": "npm audit && npm run license-validate",
    "deploy:staging": "echo 'Deployment handled by GitHub Actions'",
    "deploy:production": "echo 'Deployment handled by GitHub Actions'",
    "cicd:setup": "tsx scripts/setup-cicd.ts",
    "cicd:verify": "tsx scripts/verify-cicd.ts",
    "cicd:health-check": "tsx scripts/deployment-health-check.ts",
    "deploy:health-check:staging": "APP_URL=https://staging-n8n-converter.vercel.app tsx scripts/deployment-health-check.ts",
    "deploy:health-check:production": "APP_URL=https://n8n-converter.com tsx scripts/deployment-health-check.ts",
    "scaling:setup": "tsx scripts/scaling-setup.ts",
    "scaling:setup:staging": "tsx scripts/scaling-setup.ts staging",
    "scaling:setup:production": "tsx scripts/scaling-setup.ts production",
    "scaling:setup:docker": "tsx scripts/scaling-setup.ts production docker",
    "scaling:setup:k8s": "tsx scripts/scaling-setup.ts production kubernetes",
    "test:performance": "tsx scripts/performance-test.ts",
    "test:performance:staging": "tsx scripts/performance-test.ts https://staging-n8n-converter.vercel.app",
    "test:performance:production": "tsx scripts/performance-test.ts https://n8n-converter.com",
    "test:load": "tsx scripts/performance-test.ts http://localhost:3000 60 25",
    "test:stress": "tsx scripts/performance-test.ts http://localhost:3000 120 100",
    "setup": "node scripts/setup.sh",
    "setup:standalone": "node scripts/setup.sh --type standalone",
    "setup:docker": "node scripts/setup.sh --docker",
    "validate-env": "node scripts/validate-env.js",
    "validate-system": "node scripts/system-requirements.js",
    "generate-env": "node scripts/generate-env-template.js",
    "backup": "node scripts/backup-restore.js backup",
    "restore": "node scripts/backup-restore.js restore",
    "migrate": "node scripts/migrate.js",
    "migrate:standalone": "node scripts/migrate.js --install-cli --create-buckets"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
